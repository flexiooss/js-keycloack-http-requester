{
  "name": "@flexio-oss/js-keycloack-http-requester",
  "version": "1.0.0-dev",
  "description": "Generic asynchronous Http requester with Keycloack implementation",
  "main": "index.js",
  "module": "index.js",
  "repository": "https://github.com/flexiooss/js-keycloack-http-requester.git",
  "author": "Thomas Chatelain (https://github.com/TomAchT)",
  "license": "Apache-2.0",
  "dependencies": {
    "@flexio-corp/js-common-parent": "0.1.0",
    "@flexio-oss/xmlhttp-requester": "0.2.0"
  },
  "devDependencies": {
    "code-altimeter-js": "https://github.com/flexiooss/code-altimeter-js.git"
  },
  "peerDependencies": {
    "keycloak-js": "8.0.0"
  },
  "hotballoon-shed": {
    "build": {
      "builder": "webpack4"
    },
    "test": {
      "tester": "code-altimeter-js",
      "path": "src/test"
    },
    "generate-sources": {
      "value-objects": {
        "extension": ".spec"
      }
    }
  }
}
